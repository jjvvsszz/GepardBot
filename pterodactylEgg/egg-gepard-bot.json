            "script": "#!\/bin\/bash\r\n# Script de Instala\u00e7\u00e3o Autom\u00e1tica via GitHub Releases\r\n\r\napt-get update -y && apt-get install -y curl jq\r\n\r\nif [ -z \"${GITHUB_USER_REPO}\" ] || [ -z \"${GITHUB_PAT}\" ]; then\r\n    echo -e \"\u26a0\ufe0f  Vari\u00e1veis do GitHub n\u00e3o configuradas. Pulando download autom\u00e1tico.\"\r\nelse\r\n    echo -e \"\u2b07\ufe0f  Buscando release em ${GITHUB_USER_REPO}...\"\r\n    # Busca URL do JAR\r\n    ASSET_URL=$(curl -H \"Authorization: token ${GITHUB_PAT}\" -s \"https:\/\/api.github.com\/repos\/${GITHUB_USER_REPO}\/releases\/latest\" | jq -r '.assets[] | select(.name | endswith(\".jar\")) | .url' | head -n 1)\r\n\r\n    if [ -z \"${ASSET_URL}\" ] || [ \"${ASSET_URL}\" == \"null\" ]; then\r\n        echo -e \"\u274c Nenhum JAR encontrado na \u00faltima release.\"\r\n    else\r\n        echo -e \"\ud83d\udd17 Baixando JAR...\"\r\n        curl -L -H \"Authorization: token ${GITHUB_PAT}\" -H \"Accept: application\/octet-stream\" -o \"${SERVER_JARFILE}\" \"${ASSET_URL}\"\r\n        echo -e \"\u2705 Download conclu\u00eddo!\"\r\n    fi\r\nfi",
            "container": "debian:bookworm-slim",
            "entrypoint": "bash"
        }
